{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/[tenant]/"
    },
    {
      "route": "/_next/*",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/_next/*"]
  }
}
